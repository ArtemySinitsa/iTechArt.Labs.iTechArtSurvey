            $(document).ready(function () {
                function get_active_position() {
                    var line = $('.tmpl_hh_navigation_line'),
                        active_elem = $('.tmpl_hh_navigation ul li a.tmpl_hh_active'),
                        backgroun_position = -1228,
                        active_elem_position;

