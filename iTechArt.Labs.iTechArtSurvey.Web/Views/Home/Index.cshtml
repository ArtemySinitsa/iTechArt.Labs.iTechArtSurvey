                    menu_container.css('width', template_width + 'px');
                }

                resize_menu_container();

                var start_pos = $('.tmpl_hh_fixed_container').offset().top - 40;
                $(window).scroll(function () {
                    if ($(window).scrollTop() >= start_pos) {
                        if ($('.tmpl_hh_fixed_container').hasClass('tmpl_hh_fixed_top') == false) {
                            $('.tmpl_hh_fixed_container').addClass('tmpl_hh_fixed_top');
                            $('.tmpl_hh-wrapper').addClass('tmpl_hh_menu_fixed');
                        }
                    } else {
                        $('.tmpl_hh_fixed_container').removeClass('tmpl_hh_fixed_top');
                        $('.tmpl_hh-wrapper').removeClass('tmpl_hh_menu_fixed');
                    }

                    resize_menu_container();
                });

                function scrollToBlock(block) {
                    destination = $(block).offset().top;
                    setTimeout(function () {
                        $("body,html").animate({
