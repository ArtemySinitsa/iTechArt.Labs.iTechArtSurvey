                    line.css('background-position', backgroun_position + active_elem_position + 'px');
                }

                function tmpl_hh_toggle_page(page) {
                    $('.tmpl_hh_tab').removeClass('tmpl_hh_active_tab');
                    $('.tmpl_hh_tab_' + page).addClass('tmpl_hh_active_tab');
                    $('.tmpl_hh_navigation ul li a').removeClass('tmpl_hh_active');
                    $('.tmpl_hh_navigation ul li a[href="#' + page + '"]').addClass('tmpl_hh_active');

                    get_active_position();
                }

                $('.tmpl_hh_navigation ul li a').click(function () {
                    var page = $(this).attr('href').substr(1);
                    tmpl_hh_toggle_page(page);
                    scrollToBlock('.tmpl_hh_head_tab_container');
                });

                hash = window.location.hash;

                if (hash != '') {
                    short_hash = hash.substr(1);
                    tmpl_hh_toggle_page(short_hash);
                }

                var templateWidth = jQuery('.tmpl_hh-wrapper').width();
                if (templateWidth <= 1100 && templateWidth > 900) jQuery('.tmpl_hh-wrapper').addClass('tmpl_hh-1000');
                if (templateWidth <= 900) jQuery('.tmpl_hh-wrapper').addClass('tmpl_hh-small');

                get_active_position();

                jQuery(window).resize(function () {
                    templateWidth = jQuery('.tmpl_hh-wrapper').width();
                    if (templateWidth <= 1100 && templateWidth > 900) {
                        jQuery('.tmpl_hh-wrapper').removeClass('tmpl_hh-1000');
                        jQuery('.tmpl_hh-wrapper').removeClass('tmpl_hh-small');
                        jQuery('.tmpl_hh-wrapper').addClass('tmpl_hh-1000');
                    } else if (templateWidth <= 900) {
                        jQuery('.tmpl_hh-wrapper').removeClass('tmpl_hh-1000');
                        jQuery('.tmpl_hh-wrapper').removeClass('tmpl_hh-small');
                        jQuery('.tmpl_hh-wrapper').addClass('tmpl_hh-small');
                    } else {
                        jQuery('.tmpl_hh-wrapper').removeClass('tmpl_hh-1000');
                        jQuery('.tmpl_hh-wrapper').removeClass('tmpl_hh-small');
                    }
                    get_active_position();
                    resize_menu_container();
                });

